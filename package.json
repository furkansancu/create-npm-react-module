  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "chalk": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.2",
